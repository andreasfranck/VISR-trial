<?xml version="1.0" encoding="utf-8"?>
<!-- Generic stero configuration with left and right speakers routed to channels 1 and 2. --> 
<panningConfiguration dimension="2" infinite="false">
   <loudspeaker channel="19" id="1">
      <polar az="30" el="0" r="1"/>
   </loudspeaker>
   <loudspeaker channel="26" id="2">
      <polar az="-30" el="0" r="1"/>
   </loudspeaker>
   <triplet l1="1" l2="2"/>
</panningConfiguration>
