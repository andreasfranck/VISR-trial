<panningConfiguration dimension="2">
  <!--Defined in ITU-R BS-2051 as system B (5.1), one subwoofer.-->
  <loudspeaker id="M+030" channel="1">
    <polar az="30.0" r="1.0"/>
  </loudspeaker>
  <loudspeaker id="M-030" channel="2">
    <polar az="-30.0" r="1.0"/>
  </loudspeaker>
  <loudspeaker id="M+000" channel="3">
    <polar az="0.0" r="1.0"/>
  </loudspeaker>
  <loudspeaker id="M+110" channel="5">
    <polar az="110.0" r="1.0"/>
  </loudspeaker>
  <loudspeaker id="M-110" channel="6">
    <polar az="-110.0" r="1.0"/>
  </loudspeaker>
  <triplet l1="M-110" l2="M+110"/>
  <triplet l1="M-030" l2="M+000"/>
  <triplet l1="M-030" l2="M-110"/>
  <triplet l1="M+030" l2="M+110"/>
  <triplet l1="M+030" l2="M+000"/>
  <subwoofer name="subName" channel="4" assignedLoudspeakers="M+030, M-030, M+000, M+110, M-110"/>
</panningConfiguration>
